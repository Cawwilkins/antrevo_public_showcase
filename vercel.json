{
    "redirects": [
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "www.antrevo.com" }],
            "destination": "https://antrevo.com/:path*",
            "permanent":true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" }
            ]
        }
    ]
}